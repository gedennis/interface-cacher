{
  "name": "interface-cacher",
  "version": "0.3.0",
  "description": "A simple interface cacher based on ioredis",
  "main": "./lib/cache.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "doc": "documentation readme lib/cache.js --section=JSDoc",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc ava -v",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gedennis/interface-cacher.git"
  },
  "keywords": [
    "cacher",
    "interface",
    "redis",
    "ioredis"
  ],
  "author": "dennis.ge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gedennis/interface-cacher/issues"
  },
  "homepage": "https://github.com/gedennis/interface-cacher#readme",
  "dependencies": {
    "lodash.defaultsdeep": "^4.6.0",
    "lodash.isundefined": "^3.0.1"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "bluebird": "^3.5.1",
    "documentation": "^5.3.3",
    "ioredis": "^3.2.1",
    "coveralls": "^2.13.1",
    "lodash": "^4.17.4",
    "nyc": "^11.1.0",
    "standard-version": "^4.2.0"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "concurrency": 25,
    "serial": true,
    "failFast": false
  }
}
