    "lodash.isundefined": "^3.0.1",
    "bluebird": "^3.5.3"
  },
  "devDependencies": {
    "@types/ioredis": "^4.0.4",
    "ava": "^0.25.0",
    "coveralls": "^3.0.2",
    "documentation": "^12.0.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "ioredis": "^4.2.0",
